
<!doctype html>
<!--[if IE 9]> <html class="ie9 supports-no-js" lang="en"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html class="supports-no-js" lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="description" content="Demo store for the Simple ecommerce">

    <title>
        @ViewBag.Title
    </title>
    <link href="/assets/client/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/assets/admin/libs/jqueri-ui/themes/smoothness/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/assets/client/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/assets/admin/libs/toastr/toastr.min.css" rel="stylesheet" />
    <link href="~/assets/client/font-awesome-4.6.3/css/font-awesome.min.css" rel="stylesheet" />
    @RenderSection("headerCSS", required: false)
</head>



<body id="beautytheme" class="template-index">

    <a class="in-page-link visually-hidden skip-link" href="#MainContent">Skip to content</a>

    <div class="site-wrapper">

        <div id="quanstore-section-header" class="quanstore-section">
            <style>
                .site-header__logo {
                    width: 200px;
                }
            </style>

            <div class="top-bar grid">
                <!-- Search bar -->
                <div class="grid__item medium-up--one-fifth small--one-half">
                    <div class="top-bar__search">
                        <a href="javascript:void(0)" class="medium-up--hide">
                            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-search" viewBox="0 0 20 20"><path fill="#444" d="M18.64 17.02l-5.31-5.31c.81-1.08 1.26-2.43 1.26-3.87C14.5 4.06 11.44 1 7.75 1S1 4.06 1 7.75s3.06 6.75 6.75 6.75c1.44 0 2.79-.45 3.87-1.26l5.31 5.31c.45.45 1.26.54 1.71.09.45-.36.45-1.17 0-1.62zM3.25 7.75c0-2.52 1.98-4.5 4.5-4.5s4.5 1.98 4.5 4.5-1.98 4.5-4.5 4.5-4.5-1.98-4.5-4.5z" /></svg>
                        </a>

                        <form action="/tim-kiem" method="get" class="search-bar small--hide" role="search">

                            <button type="submit" id="btnSearch" class="search-bar__submit">
                                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-search" viewBox="0 0 20 20"><path fill="#444" d="M18.64 17.02l-5.31-5.31c.81-1.08 1.26-2.43 1.26-3.87C14.5 4.06 11.44 1 7.75 1S1 4.06 1 7.75s3.06 6.75 6.75 6.75c1.44 0 2.79-.45 3.87-1.26l5.31 5.31c.45.45 1.26.54 1.71.09.45-.36.45-1.17 0-1.62zM3.25 7.75c0-2.52 1.98-4.5 4.5-4.5s4.5 1.98 4.5 4.5-1.98 4.5-4.5 4.5-4.5-1.98-4.5-4.5z" /></svg>
                                <span class="icon__fallback-text">Search</span>
                            </button>
                            <input type="search" id="txtKeyword" name="keyword" class="search-bar__input" value="" placeholder="Search" aria-label="Search">
                        </form>
                    </div>
                </div>
                <!-- Menu -->
                @{ Html.RenderAction("Header", "Home"); }
            </div>

            <hr class="small--hide hr--border">
            <header class="site-header grid medium-up--grid--table" role="banner">
                <div class="grid__item small--text-center">
                    <div itemscope itemtype="http://schema.org/Organization">


                        <a href="/" itemprop="url">

                            <img class="site-header__logo" src="//cdn.shopify.com/s/files/1/0254/6101/files/logo-horizontal-B_200x.png?v=1480351276"
                                 srcset="//cdn.shopify.com/s/files/1/0254/6101/files/logo-horizontal-B_200x.png?v=1480351276 1x, //cdn.shopify.com/s/files/1/0254/6101/files/logo-horizontal-B_200x@2x.png?v=1480351276 2x"
                                 alt="beautytheme"
                                 itemprop="logo">
                        </a>

                        <h1 class="visually-hidden">beautytheme</h1>


                    </div>
                </div>

            </header>
        </div>

        <div class="grid">
            <div id="quanstore-section-sidebar" class="quanstore-section">
                <div data-section-id="sidebar" data-section-type="sidebar-section">
                    <nav class="grid__item small--text-center medium-up--one-fifth" role="navigation">
                        <hr class="hr--small medium-up--hide">
                        <button id="ToggleMobileMenu" class="mobile-menu-icon medium-up--hide" aria-haspopup="true" aria-owns="SiteNav">
                            <span class="line"></span>
                            <span class="line"></span>
                            <span class="line"></span>
                            <span class="line"></span>
                            <span class="icon__fallback-text">Menu</span>
                        </button>

                        <!-- Category -->
                        @{ Html.RenderAction("Menu", "Home"); }

                    </nav>

                </div>








            </div>

            <main class="main-content grid__item medium-up--four-fifths" id="MainContent" role="main">


                @RenderBody()

            </main>

        </div>

        <hr>

        <div id="quanstore-section-footer" class="quanstore-section">
            @{ Html.RenderAction("Footer", "Home"); }
        </div>

    </div>



    <script src="~/assets/client/js/jquery.min.js"></script>
    <script src="~/assets/admin/libs/toastr/toastr.min.js"></script>
    <script src="~/assets/admin/libs/jqueri-ui/jquery-ui.min.js"></script>
    <script src="~/assets/client/js/owl.carousel.min.js"></script>
    <script src="/assets/client/js/theme.js" type="text/javascript"></script>
    
   
   
   

    @RenderSection("footerJS", required: false)
</body>
</html>
<noembed> 
